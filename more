src/gui/GUIEvent_SimulationEnded.h:/**
src/gui/GUIEvent_SimulationEnded.h: * @class GUIEvent_SimulationEnded
src/gui/GUIEvent_SimulationEnded.h: * @brief Event sent when the the simulation is over
src/gui/GUIEvent_SimulationEnded.h: *
src/gui/GUIEvent_SimulationEnded.h: * Throw from GUIRunThread to GUIApplicationWindow.
src/gui/GUIEvent_SimulationEnded.h: */
src/gui/GUIEvent_SimulationEnded.h:class GUIEvent_SimulationEnded : public GUIEvent {
src/gui/GUIEvent_SimulationEnded.h:public:
src/gui/GUIEvent_SimulationEnded.h:    /** @brief Constructor
src/gui/GUIEvent_SimulationEnded.h:     * @param[in] reason The reason the simulation has ended
src/gui/GUIEvent_SimulationEnded.h:     * @param[in] step The time step the simulation has ended at
src/gui/GUIEvent_SimulationEnded.h:     */
src/gui/GUIEvent_SimulationEnded.h:    GUIEvent_SimulationEnded(MSNet::SimulationState reason, SUMOTime step)
src/gui/GUIEvent_SimulationEnded.h:        : GUIEvent(EVENT_SIMULATION_ENDED), myReason(reason), myStep(step) {}
src/gui/GUIEvent_SimulationEnded.h:    /// @brief Destructor
src/gui/GUIEvent_SimulationEnded.h:    ~GUIEvent_SimulationEnded() { }
src/gui/GUIEvent_SimulationEnded.h:    /** @brief Returns the time step the simulation has ended at
src/gui/GUIEvent_SimulationEnded.h:     * @return The time step the simulation has ended at
src/gui/GUIEvent_SimulationEnded.h:     */
src/gui/GUIEvent_SimulationEnded.h:    SUMOTime getTimeStep() const {
src/gui/GUIEvent_SimulationEnded.h:        return myStep;
src/gui/GUIEvent_SimulationEnded.h:    }
src/gui/GUIEvent_SimulationEnded.h:    /** @brief Returns the reason the simulation has ended due
src/gui/GUIEvent_SimulationEnded.h:     * @return The reason the simulation has ended
src/gui/GUIEvent_SimulationEnded.h:     */
src/gui/GUIEvent_SimulationEnded.h:    MSNet::SimulationState getReason() const {
src/gui/GUIEvent_SimulationEnded.h:        return myReason;
src/gui/GUIEvent_SimulationEnded.h:    }
src/gui/GUIEvent_SimulationEnded.h:protected:
src/gui/GUIEvent_SimulationEnded.h:    /// @brief The reason the simulation has ended
src/gui/GUIEvent_SimulationEnded.h:    MSNet::SimulationState myReason;
src/gui/GUIEvent_SimulationEnded.h:    /// @brief The time step the simulation has ended at
src/gui/GUIEvent_SimulationEnded.h:    SUMOTime myStep;
src/gui/GUIEvent_SimulationEnded.h:};
src/gui/GUIEvent_SimulationEnded.h:#endif
