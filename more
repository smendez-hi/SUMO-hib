src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    int id;
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    from >> id;
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    std::string tag;
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    from >> tag;
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    std::string name = readName(from);
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    // parse the districts
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    //  and allocate them if not done before
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    //  A district may be already saved when another parking place with
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    //  the same district was already build.
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    IntVector districts;
src/netimport/vissim/typeloader/NIVissimSingleTypeParser_Parkplatzdefinition.cpp:    DoubleVector percentages;
